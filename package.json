{
    "name": "epi-catch-up",
    "description": "Build an EPI catch-up app to be integrated into your DHIS2 instance.",
    "homepage": ".",
    "version": "0.1.0",
    "private": true,
    "developer": {
        "name": "Andreas Rey"
    },
    "license": "MIT",
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "48": "favicon-48x48.png"
    },
    "manifest.webapp": {
        "name": "EPI catch-up app",
        "description": "Do something cool.",
        "appType": "APP",
        "activities": {
            "dhis": {
                "href": "*"
            }
        }
    },
    "scripts": {
        "start": "react-scripts start",
        "prebuild": "rimraf build",
        "build": "react-scripts build && d2-manifest package.json build/manifest.webapp",
        "postbuild": "rimraf epi-catch-up.zip && cd build && bestzip ../epi-monitoring-map.zip *",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@dhis2/app-runtime": "^2.0.1",
        "@dhis2/gis-api": "^34.0.10",
        "@dhis2/ui-core": "^3.11.0",
        "@dhis2/ui-widgets": "^1.0.8",
        "@material-ui/core": "^4.9.0",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/styles": "^4.9.0",
        "@observablehq/stdlib": "^3.4.0",
        "@turf/helpers": "^6.1.4",
        "@turf/point-on-feature": "^5.1.5",
        "@turf/turf": "^5.1.6",
        "chart.js": "^2.9.3",
        "d2": "^31.8.1",
        "d3": "^6.2.0",
        "file-saver": "^2.0.2",
        "html2canvas": "^1.0.0-rc.7",
        "jquery": "^3.4.1",
        "moment": "^2.25.3",
        "react": "^16.9.0",
        "react-chartjs-2": "^2.9.0",
        "react-color": "^2.18.1",
        "react-dom": "^16.9.0",
        "react-redux": "^5.0.7",
        "redux": "^4.0.5",
        "typeface-roboto": "^0.0.75"
    },
    "devDependencies": {
        "@dhis2/cli-style": "^4.1.1",
        "bestzip": "^2.1.4",
        "d2-manifest": "^1.0.0",
        "react-scripts": "^3.1.2",
        "rimraf": "^3.0.0"
    }
}
